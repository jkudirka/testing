<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="filePath" value="C:\tmp\UserStore.dat"/>
  </appSettings>
  <system.serviceModel>
    <bindings>
      <netNamedPipeBinding>
        <binding name="UserManagerBinding" transactionFlow="true" />
      </netNamedPipeBinding>
    </bindings>
    <client>
      <endpoint address="net.pipe://localhost/UserManager" binding="netNamedPipeBinding"
        bindingConfiguration="UserManagerBinding" contract="DataContracts.IUserManager"
        name="UserManagerService" />
    </client>
  </system.serviceModel>
</configuration>