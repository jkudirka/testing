<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="filePath" value="C:\tmp\UserStore.dat"/>
  </appSettings>
  <system.serviceModel>
    <bindings>
      <netNamedPipeBinding>
        <binding name="UserManagerBinding" transactionFlow="true" />
      </netNamedPipeBinding>
    </bindings>
    <services>
      <service name="UserManager">
        <endpoint address="net.pipe://localhost/UserManager" binding="netNamedPipeBinding"
          bindingConfiguration="UserManagerBinding" name="UserManagerService"
          contract="DataContracts.IUserManager" />
      </service>
    </services>
  </system.serviceModel>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>